{
  "name": "@dhunanyan/scss-to-css-converter",
  "author": "dhunanyan",
  "license": "MIT",
  "description": "This project is a tool designed to make converting SCSS (Sassy CSS) code to CSS (Cascading Style Sheets) simple and accessible. With this converter, you can work with SCSS syntax and quickly generate the final CSS code for easy deployment and browser compatibility.",
  "keywords": [
    "scss",
    "css",
    "scss-to-css",
    "sass",
    "scss compiler",
    "css compiler",
    "scss converter",
    "css converter",
    "preprocessor",
    "css preprocessor",
    "sass compiler",
    "css processor",
    "frontend",
    "stylesheets",
    "web development",
    "css transformation",
    "style converter",
    "scss processor",
    "node-sass",
    "postcss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhunanyan/scss-to-css-converter-npm-package.git"
  },
  "bugs": {
    "url": "https://github.com/dhunanyan/scss-to-css-converter-npm-package/issues"
  },
  "homepage": "https://github.com/dhunanyan/scss-to-css-converter-npm-package#readme",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "version": "3.3.1",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "eslint ./*.ts --fix",
    "build:converter": "bash scripts/build_converter.sh",
    "build:package": "tsc --jsx react --target es6 --lib es2021,dom",
    "publish": "npm publish --access=public",
    "prepare": "husky"
  },
  "dependencies": {
    "antlr4": "^4.13.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.6",
    "typescript": "^5.6.3"
  }
}
